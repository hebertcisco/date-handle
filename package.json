{
  "name": "date-handle",
  "version": "0.0.1",
  "description": "Handle with dates on JS and TS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/date-handle.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/date-handle/issues"
  },
  "homepage": "https://github.com/hebertcisco/date-handle#readme",
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/moment": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "8.5.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "27.4.5",
    "ts-jest": "27.1.2",
    "typescript": "^4.0.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "moment": "^2.29.1"
  }
}